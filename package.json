{
  "name": "ZimmerNameGenerator",
  "version": "0.2.0",
  "description": "Zimmer Name Generator",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --hot --config ./webpack.dev.config.js",
    "build": "webpack --progress --colors --config ./webpack.prod.config.js",
    "serve": "serve docs/",
    "publish": "npm version $1 -m \"Publish version %s\"",
    "post-publish": "git push && git push --tags",
    "test": "NODE_ENV=test nyc -s babel-tape-runner test/**/*.test.js | tap-spec && nyc report",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yotambarzilay/zimmer.git"
  },
  "keywords": [],
  "author": "yotambarzilay",
  "license": "ISC",
  "engines": {
    "node": "6.6.0",
    "npm": "3.10.6"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.18.0",
    "babel-tape-runner": "^2.0.1",
    "coveralls": "^2.11.15",
    "css-loader": "^0.25.0",
    "extend-tape": "^1.2.0",
    "file-loader": "^0.9.0",
    "firebase": "^3.6.4",
    "lodash": "^4.17.2",
    "mobx": "^2.7.0",
    "mobx-react": "^4.0.3",
    "node-sass": "^3.10.0",
    "nyc": "^10.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.0",
    "react-unit": "^2.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-jsx-equals": "^1.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
